{
    "name": "unocha/starterkit",
    "description": "Starterkit",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "UNOCHA",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=8.0 || ^7.3",
        "composer/installers": "^1.10",
        "cweagans/composer-patches": "^1.7",
        "drupal-composer/preserve-paths": "^0.1.6",
        "drupal/admin_denied": "^1.1",
        "drupal/admin_toolbar": "^3.0",
        "drupal/auto_config_form": "1.0.x-dev@dev",
        "drupal/bootstrap": "^3.23",
        "drupal/bootstrap_barrio": "5.1.5",
        "drupal/ckeditor_config": "^3.1",
        "drupal/ckeditor_font": "^1.2",
        "drupal/colorbutton": "^1.3",
        "drupal/component_library": "1.x-dev@dev",
        "drupal/components": "^3.0@beta",
        "drupal/core-composer-scaffold": "^9.2.7",
        "drupal/core-dev": "^9.2.7",
        "drupal/core-project-message": "^9.2",
        "drupal/core-recommended": "9.2.7",
        "drupal/ctools": "^3.7",
        "drupal/ds": "^3.13",
        "drupal/dxpr_theme": "^1.2",
        "drupal/entity_browser": "^2.6",
        "drupal/environment_indicator": "^4.0",
        "drupal/features": "^3.12",
        "drupal/genpass": "^1.1",
        "drupal/google_analytics": "^3.1",
        "drupal/google_tag": "^1.4",
        "drupal/http_client_manager": "^2.5",
        "drupal/imce": "^2.4",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jquery_ui": "^1.4",
        "drupal/libraries": "^3.0@beta",
        "drupal/login_destination": "^2.0@beta",
        "drupal/maintenance200": "^1.1",
        "drupal/media_entity_browser": "^2.0@alpha",
        "drupal/menu_block": "^1.7",
        "drupal/menu_breadcrumb": "^1.16",
        "drupal/menu_entity_index": "^1.3",
        "drupal/menu_firstchild": "^2.0",
        "drupal/menu_link_attributes": "^1.2",
        "drupal/metatag": "^1.16",
        "drupal/migrate_file": "^2.0",
        "drupal/migrate_plus": "^5.1",
        "drupal/migrate_tools": "^5.0",
        "drupal/migrate_upgrade": "^3.2",
        "drupal/modules_weight": "^1.9",
        "drupal/noreqnewpass": "^1.2",
        "drupal/override_node_options": "^2.6",
        "drupal/pathauto": "^1.8",
        "drupal/pathologic": "^1.0@alpha",
        "drupal/redirect": "^1.6",
        "drupal/redis": "^1.5",
        "drupal/seckit": "^2.0",
        "drupal/semanticviews": "^2.3",
        "drupal/social_auth": "^3.0",
        "drupal/social_auth_hid": "^2.7",
        "drupal/stage_file_proxy": "^1.1",
        "drupal/superfish": "^1.4",
        "drupal/tb_megamenu": "^1.3",
        "drupal/token": "^1.9",
        "drupal/token_filter": "^1.2",
        "drupal/tome": "^1.4",
        "drupal/userprotect": "^1.1",
        "drupal/video_embed_field": "^2.4",
        "drupal/views_base_url": "^2.0@beta",
        "drupal/views_block_area": "^1.0@beta",
        "drupal/votingapi": "^3.0@beta",
        "drush/drush": "^10.3",
        "mikehaertl/phpwkhtmltopdf": "^2.5",
        "mpdf/mpdf": "^8.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "symfony/flex": "^1.17",
        "unocha/common_design": "^4.0.0",
        "webflo/drupal-finder": "^1.2.2"
    },
    "require-dev": {
        "drupal/coder": "^8.3.12",
        "drupal/console": "^1.9.7",
        "drupal/devel": "^4.1.1",
        "drupal/devel_php": "^1.3",
        "phpcompatibility/php-compatibility": "^9.3.5",
        "phpmd/phpmd": "^2.9.1",
        "phpunit/phpunit": "^9.5.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "bin-dir": "vendor/bin/",
        "sort-packages": true
    },
    "extra": {
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "html/core": ["type:drupal-core"],
            "html/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "html/modules/contrib/{$name}": ["type:drupal-module"],
            "html/profiles/contrib/{$name}": ["type:drupal-profile"],
            "html/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php",
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            },
            "locations": {
              "web-root": "html/"
            },
            "omit-defaults": false
        },
        "merge-plugin": {
            "include": [
                "html/core/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "merge-extra": false
        },
        "preserve-paths": [
            "html/modules/custom",
            "html/themes/custom",
            "html/sites/default"
        ],
        "patches-file": "composer.patches.json",
        "composer-exit-on-patch-failure": true
    },
    "scripts": {
        "git-hooks": "git config core.hooksPath git-hooks || exit 0",
        "sass-prepare": "cd html/themes/custom/gms_ocha_common && npm install",
        "sass-build": "cd html/themes/custom/gms_ocha_common && ./node_modules/.bin/sass sass:build",
        "sass-cleanup": "cd html/themes/custom/gms_ocha_common && rm -rf ./node_modules",
        "sass": [
            "@sass-prepare",
            "@sass-build",
            "@sass-cleanup"
        ],
        "post-install-cmd": [
            "@git-hooks"
        ],
        "post-update-cmd": [
            "@git-hooks"
        ]
    }
}
