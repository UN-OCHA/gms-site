    "require-dev": {
        "drupal/coder": "^8.3.12",
        "drupal/console": "^1.9.7",
        "drupal/devel": "^5.0.2",
        "drupal/devel_php": "^1.3",
        "drupal/stage_file_proxy": "^2.0",
        "phpcompatibility/php-compatibility": "^9.3.5",
        "phpmd/phpmd": "^2.9.1",
        "phpunit/php-code-coverage": "^9.2",
        "phpunit/phpunit": "^9.5",
        "weitzman/drupal-test-traits": "^2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "bin-dir": "vendor/bin/",
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/console-extend-plugin": true,
            "oomphinc/composer-installers-extender": true,
            "symfony/flex": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "extra": {
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "html/core": ["type:drupal-core"],
            "html/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "html/modules/contrib/{$name}": ["type:drupal-module"],
            "html/profiles/contrib/{$name}": ["type:drupal-profile"],
            "html/themes/contrib/{$name}": ["type:drupal-theme"],
            "html/modules/custom/{$name}": ["type:drupal-custom-module"],
            "html/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php",
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            },
            "locations": {
              "web-root": "html/"
            },
            "omit-defaults": false
        },
        "merge-plugin": {
            "include": [
                "html/core/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "merge-extra": false
        },
        "patches-file": "composer.patches.json",
        "composer-exit-on-patch-failure": true
    },
    "scripts": {
        "git-hooks": "git config core.hooksPath git-hooks || exit 0",
        "sass-prepare": "cd html/themes/custom/gms_ocha_common && npm install",
        "sass-build": "cd html/themes/custom/gms_ocha_common && ./node_modules/.bin/sass sass:build",
        "sass-cleanup": "cd html/themes/custom/gms_ocha_common && rm -rf ./node_modules",
        "sass": [
            "@sass-prepare",
            "@sass-build",
            "@sass-cleanup"
        ],
        "post-install-cmd": [
            "@git-hooks"
        ],
        "post-update-cmd": [
            "@git-hooks"
        ]
    }
}
